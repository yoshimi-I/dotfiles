{
  "[javascript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.ruff": "explicit",
      "source.organizeImports.ruff": "explicit"
    }
  },
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "cSpell.userWords": ["chatgpt", "Monokai", "techtrain"],
  "editor.acceptSuggestionOnEnter": "on",
  "editor.accessibilitySupport": "off",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.dragAndDrop": false,
  "editor.fontSize": 12,
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.preview": true,
  "editor.suggest.showMethods": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "erb": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.variables": {
    "lang": "ja"
  },
  "files.associations": {
    "*.erb": "erb"
  },
  "files.autoGuessEncoding": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "jupyter.experimental.debugging": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "python.linting.flake8Args": ["--ignore=E501"],
  "python.linting.flake8Enabled": true,
  "techtrain-railway.optOutTelemetry": false,
  "terminal.external.osxExec": "warp.app",
  "terminal.integrated.inheritEnv": false,
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "AurulentSansM Nerd Font",
  "hediet.vscode-drawio.resizeImages": null,
  "git.suggestSmartCommit": false,
  "notebook.lineNumbers": "on",
  "files.autoSave": "afterDelay",
  "diffEditor.ignoreTrimWhitespace": false,
  "hediet.vscode-drawio.theme": "Kennedy",
  "editor.formatOnSave": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "genieai.promptPrefix.addComments": "このコードに日本語のコメントを追加してください。",
  "genieai.promptPrefix.addTests": "このコードのテストを実装してください。",
  "genieai.promptPrefix.completeCode": "このコードを完成させてください。",
  "genieai.promptPrefix.explain": "このコードを日本語で説明をしてください。",
  "genieai.promptPrefix.findProblems": "このコードのバグを見つけてください。",
  "genieai.promptPrefix.optimize": "このコードを最適化してください。",
  "explorer.confirmDragAndDrop": false,
  "github.copilot.advanced": {},
  "remote.autoForwardPortsSource": "hybrid",
  "window.zoomLevel": 1,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "/Users/yoshimi/.vimrc",
  "vim.disableAnnoyingNeovimMessage": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.ignorecase": true,
  "vim.smartcase": true,
  "vim.useSystemClipboard": true,
  "vim.leader": " ",
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["x"],
      "after": ["\"", "_", "x"]
    },
    {
      "before": ["s"],
      "after": ["\"", "_", "s"]
    },
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": [":"],
      "after": [";"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["<leader>", "h"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["<C-w>", "j"]
    },
    {
      "before": ["<leader>", "k"],
      "after": ["<C-w>", "k"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["<C-w>", "l"]
    },
    {
      "before": ["["],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": [":sp"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": [":vs"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": ["<C-l>"],
      "after": ["g", "t"]
    },
    {
      "before": ["<C-h>"],
      "after": ["g", "T"]
    },
    {
      "before": ["g", "r"],
      "after": ["g", "T"]
    },
    {
      "before": ["<leader>", "o"],
      "after": ["o", "<ESC>"]
    },
    {
      "before": ["<leader>", "O"],
      "after": ["O", "<ESC>"]
    },
    {
      "before": ["n"],
      "after": ["n", "z", "z"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z"]
    },
    {
      "before": ["*"],
      "after": ["*", "z", "z"]
    },
    {
      "before": ["#"],
      "after": ["#", "z", "z"]
    },
    {
      "before": ["<C-n>"],
      "after": ["g", "b"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.insertCursorAbove"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["editor.action.insertCursorBelow"]
    }
  ]
}
