{
    "[javascript]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "[typescript]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // 以下Python
    "[python]": {
        "editor.formatOnType": true,
        "editor.codeActionsOnSave": {
            "source.fixAll.ruff": "explicit",
            "source.organizeImports.ruff": "explicit"
        }
    },
    "python.analysis.extraPaths": [
        "${workspaceFolder}",
        "${workspaceFolder}/src",
        "${workspaceFolder}/models"
    ],
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}",
        "${workspaceFolder}/src",
        "${workspaceFolder}/models"
    ],
    "python.envFile": "${workspaceFolder}/.env",
    "terminal.integrated.env.osx": {
        "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/src:${workspaceFolder}/models",
        "FIG_NEW_SESSION": "1"
    },
    "python.analysis.indexing": true,
    "python.analysis.autoSearchPaths": true,
    "python.analysis.useLibraryCodeForTypes": true,
    "jupyter.notebookFileRoot": "${workspaceFolder}",
    "[jupyter]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },

    // 以下Go
    "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnSave": true
    },
    "go.useLanguageServer": true,
    "go.toolsManagement.autoUpdate": true,
    "gopls": {
        "ui.semanticTokens": true,
        "analyses": {
            "unusedparams": true,
        },
        "staticcheck": true
    },

    "cSpell.userWords": [
        "chatgpt",
    ],
    "editor.accessibilitySupport": "off",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "PlemolJP35 Console NF, monospace",
    "editor.fontSize": 12,
    "editor.inlineSuggest.enabled": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.preview": true,
    "editor.suggest.showMethods": true,
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "emmet.includeLanguages": {
        "erb": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "emmet.variables": {
        "lang": "ja"
    },
    "workbench.colorCustomizations": {
        "statusBar.background": "#393087",
        "editor.selectionBackground": "#74939d",
        "tab.activeBackground": "#393087",
        "tab.activeForeground": "#74939d",
        // タブの色設定
        "editorIndentGuide.background1": "#b22222",
        "editorIndentGuide.background2": "#ff4500",
        "editorIndentGuide.background3": "#ffd700",
        "editorIndentGuide.background4": "#228b22",
        "editorIndentGuide.background5": "#000080",
        "editorIndentGuide.background6": "#8b008b",
        "editorIndentGuide.activeBackground1": "#fff",
        "editorIndentGuide.activeBackground2": "#fff",
        "editorIndentGuide.activeBackground3": "#fff",
        "editorIndentGuide.activeBackground4": "#fff",
        "editorIndentGuide.activeBackground5": "#fff",
        "editorIndentGuide.activeBackground6": "#fff"

    },
    "files.associations": {
        "*.erb": "erb"
    },
    "files.autoGuessEncoding": true,
    "files.eol": "\n",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "terminal.external.osxExec": "warp.app",
    "terminal.integrated.inheritEnv": false,
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
    "workbench.colorTheme": "One Monokai",
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.fontFamily": "AurulentSansM Nerd Font",
    "hediet.vscode-drawio.resizeImages": null,
    "git.suggestSmartCommit": false,
    "notebook.lineNumbers": "on",
    "files.autoSave": "afterDelay",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnSave": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "explorer.confirmDragAndDrop": false,
    "github.copilot.advanced": {},
    "remote.autoForwardPortsSource": "hybrid",
    "window.zoomLevel": 1,
    // Cursorのアクティビティバーの向きを垂直にする
    "workbench.activityBar.orientation": "vertical",
    // 以下vimの設定
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "/Users/yoshimi/.vimrc",
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.ignorecase": true,
    "vim.smartcase": true,
   "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.leader": " ",
    "vim.handleKeys": {
        "<C-c>": false,
        "<C-v>": false,
        "<C-x>": false
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "j"
            ],
            "commands": [
               "workbench.action.moveEditorToPreviousGroup"
            ]
          },
          {
            "before": [
                "<leader>",
                "k"
            ],
            "commands": [
              "workbench.action.moveEditorToNextGroup"
            ]
          },
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "g",
                "t"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "g",
                "T"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-w>",
                "h"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-w>",
                "l"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "a"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                "workbench.action.moveActiveEditorGroupLeft"
            ]
        },
        {
            "before": [
                "<leader>",
                "l"
            ],
            "commands": [
                "workbench.action.moveActiveEditorGroupRight"
            ]
        },
        {
            "before": [
                "g",
                "u"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                "g",
                "s"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "<C-[>"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "<C-]>"
            ],
            "commands": [
                "workbench.action.navigateForward"
            ]
        },
        {
            "before": [
                "<leader>",
                "l",
                "s"
            ],
            "commands": "workbench.view.explorer"
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "q"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<C-e>"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                "workbench.action.splitEditorRight"
            ]
        },
        {
            "before": [
                "[",
                "q"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        {
            "before": [
                "]",
                "q"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "before": [
                "<C-e>"
            ],
            "commands": [
                "workbench.action.focusActiveEditorGroup"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-g>"
            ],
            "commands": [
                "editor.action.addSelectionToNextFindMatch"
            ]
        },
        {
            "before": [
                "<leader>",
                "g"
            ],
            "commands": [
                "editor.action.selectHighlights"
            ]
        }
    ],
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "database-client.autoSync": true
}
