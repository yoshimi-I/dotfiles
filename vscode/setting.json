{
  "[javascript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.ruff": "explicit",
      "source.organizeImports.ruff": "explicit"
    }
  },
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "cSpell.userWords": ["chatgpt", "Monokai", "techtrain"],
  "editor.acceptSuggesionOnEnter": "on",
  "editor.accessibilitySupport": "off",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.dragAndDrop": false,
  "editor.fontSize": 12,
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.preview": true,
  "editor.suggest.showMethods": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "erb": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.variables": {
    "lang": "ja"
  },
  "files.associations": {
    "*.erb": "erb"
  },
  "files.autoGuessEncoding": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "jupyter.experimental.debugging": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "python.linting.flake8Args": ["--ignore=E501"],
  "python.linting.flake8Enabled": true,
  "techtrain-railway.optOutTelemetry": false,
  "terminal.external.osxExec": "warp.app",
  "terminal.integrated.inheritEnv": false,
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "AurulentSansM Nerd Font",
  "hediet.vscode-drawio.resizeImages": null,
  "git.suggestSmartCommit": false,
  "notebook.lineNumbers": "on",
  "files.autoSave": "afterDelay",
  "diffEditor.ignoreTrimWhitespace": false,
  "hediet.vscode-drawio.theme": "Kennedy",
  "editor.formatOnSave": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "explorer.confirmDragAndDrop": false,
  "github.copilot.advanced": {},
  "remote.autoForwardPortsSource": "hybrid",
  "window.zoomLevel": 1,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "/Users/yoshimi/.vimrc",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.ignorecase": true,
  "vim.smartcase": true,
  "vim.useSystemClipboard": true,
  "vim.leader": " ",
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-k>"],
      "after": ["g", "t"]
    },
    {
      "before": ["<C-j>"],
      "after": ["g", "T"]
    },
    {
      "before": ["<C-h>"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-w>", "l"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f", "a"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.moveActiveEditorGroupLeft"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.moveActiveEditorGroupRight"]
    },
    {
      "before": ["g", "u"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["g", "s"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["<C-[>"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<C-]>"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<leader>", "l", "s"],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    {
      "before": ["u"],
      "commands": [
        "undo"
      ]
    },
    {
      "before": ["<leader>", "w", "q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["[", "q"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "q"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    }
  ]
}
